{
    "name": "yoozi/golem",
    "description": "Daily PHP development toolkits for Yoozi RD team.",
    "keywords": ["yoozi", "email"],
    "license": "MIT",
    "authors": [
        {
            "name": "Saturn HU",
            "email": "yangg.hu@yoozi.cn"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4.1",
        "nesbot/carbon": "~1.0"
    },
    "replace": {
        "yoozi/email": "self.version"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Yoozi": "lib/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
